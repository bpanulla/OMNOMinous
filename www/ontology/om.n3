@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix : <http://www.semanticweb.org/owl/owlapi/turtle#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://omnomino.us/ontology/om.n3> .

<http://omnomino.us/ontology/om.n3> rdf:type owl:Ontology ;
         owl:imports <http://purl.org/dc/terms/> ,
                     <http://xmlns.com/foaf/0.1/> .

<#bookmarks> rdf:type owl:ObjectProperty ;
			rdfs:domain <#Member>,
			rdfs:range <#Resource>.

<http://omnomino.us/ontology/om.n3#Member> rdf:type owl:Class ;
			rdfs:subClassOf <http://xmlns.com/foaf/0.1/Person> .

<http://omnomino.us/ontology/om.n3#Resource> rdf:type owl:Class ;
			rdfs:subClassOf <http://xmlns.com/foaf/0.1/Document> .
                                                                                  